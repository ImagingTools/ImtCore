schema {
    query: Query
    mutation: Mutation
    name: AuthorizationOptions
    namespace: imtauth
    version: 1.0
}

enum UserMode {
    NO_USER_MANAGEMENT
    OPTIONAL_USER_MANAGEMENT
    STRONG_USER_MANAGEMENT
}

type UserManagementPayload {
    UserMode: UserMode
}

input UserManagementInput {
    ProductId: ID
}

type Query {
    GetUserMode (input: UserManagementInput): UserManagementPayload!
}
