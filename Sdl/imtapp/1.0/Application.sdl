schema {
	query: Query
	namespace: imtapp
	version: 1.0
}

type ApplicationInfo {
	version: String
	companyName: String
	productName: String
	applicationId: String
	applicationName: String
	applicationType: String
}

input GetApplicationInfoInput {
	productId: ID
}

type Query {
	GetApplicationInfo (input: GetApplicationInfoInput): ApplicationInfo!
}


