schema {
	namespace: imtbase
	version: 1.0
}

enum EUndoStatus {
	InvalidUserId
	InvalidDocumentId
	Success
	InvalidStepCount
	Failed
}

type UndoStatus {
	status: EUndoStatus!
}

type UndoInfo {
	documentId: ID!
	status: UndoStatus!
	isDirty: Bool
	availableUndoSteps: Int
	availableRedoSteps: Int
	undoLevelDescriptions: [String]
	redoLevelDescriptions: [String]
}

type UndoRedoInput {
	documentId: ID!
	steps: Int!
}

