enum MeasurementType{
	NONE
	WIDTH
	BRIGHTNESS
	DISTANCE
	DISPLACEMENT
	HEIGHT
	SYMMETRY
	VOLUME
	VOLUMERATE
	LENGTH
	OUT_OF_BOUND
}

enum MeasurementUnit{
	NONE
	mm
	mm3
	mm3_mm
	pixel
}

enum GeometryType{
	LINE
	POLYGON
	CIRCLE
}

type Point{
	X: Float!
	Y: Float!
}

type Geometry{
	GeometryType: GeometryType!
	Radius: Float
	Points: [Point]!
	RequiredPoints: [Point!]!
	OptionalPoints: [Point]
}