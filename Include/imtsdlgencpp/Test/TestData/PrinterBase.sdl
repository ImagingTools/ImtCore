schema {
	name: PrinterBase
	namespace: modsdl
	version: 1.0
	query: Query
}

type PrinterSpecificationBase{
	name: String
}

type Link {
	link: ID!
}

union PrinterSpecification = PrinterSpecificationBase | Link

type PrinterBase {
	name: String
	specification: PrinterSpecification!
}

type PrinterList{
	data: [PrinterBase]
}


type Query {
	GetPrinters: PrinterList
}


