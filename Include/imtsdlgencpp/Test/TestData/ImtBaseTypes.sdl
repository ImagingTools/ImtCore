schema {
	name: ImtBaseTypes
	namespace: imtbase
	version: 1.0
}


type TimeRange
{
	Begin: String
	End: String
}

type SdlSize
{
	width: Double!
	height: Double!
}

type SdlPoint
{
	x: Double!
	y: Double!
}

type ParamTypeIds
{
	UrlParam: ID
	IdParam: ID
	TextParam: ID
	TextView: ID
	SelectionParam: ID
	SchedulerParam: ID
	BackupSettings: ID
	DatabaseAccessSettings: ID
	ParamsSet: ID
	FileNameParam: ID
	IntegerParam: ID
	DoubleParam: ID
	PasswordParam: ID
	EnableableParam: ID
}

type UrlParam
{
	scheme: String
	host: String
	port: Integer
	path: String
}

type IdParam
{
	id: ID
}

type TextParam
{
	text: String
}

type EnableableParam
{
	value: Boolean
}

type IntegerParam
{
	value: Integer
}

type DoubleParam
{
	value: Double
}

type Option
{
	id: ID
	name: String
	description: String
	enabled: Boolean
}

type OptionsList
{
	totalCount: Integer
	offset: Integer
	options: [Option]
}

type SelectionParam
{
	selectedIndex: Integer
	constraints: OptionsList
}

type SchedulerParam
{
	startTime: String
	interval: Integer
}

type BackupSettings
{
	schedulerParam: SchedulerParam
	folderPath: String
}

type DatabaseAccessSettings
{
	dbName: String
	host: String
	port: Integer
	dbPath: String
	username: String
	password: String
}

type FileNameParam
{
	pathType: Integer
	path: String
}

type ParamsSet
{
	paramIds: [ID]
	paramTypeIds: [ID]
	paramNames: [String]
	paramDescriptions: [String]
	parameters: [String]
}

type MimeType
{
	type: String!
	tree: [String]
	subType: String!
	suffix: String
	parameters: [String]
}
