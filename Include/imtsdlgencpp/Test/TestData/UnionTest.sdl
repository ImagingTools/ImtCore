schema {
	namespace: modsdl
	version: 1.0
	query: Query
}

type Coords{
	X: Double!
	Y: Double!
}

type PrinterSpecificationBase {
	name: String
}

enum LinkStatus {
	OPEN
	CLOSE
}

type Link {
	link: ID!
	status: LinkStatus
	statusList: [LinkStatus]
}

union PrinterSpecification = PrinterSpecificationBase | Link | String | Double

type PrinterBase {
	name: String
	linkList: [Link]!
	specification: PrinterSpecification!
	specificationList: [PrinterSpecification]!
}

type Query {
	GetSpecifications(input: Link): PrinterSpecification
}

collectionSchema {
	type Printer {
		ref: PrinterSpecification
		operations {
			get: GetSpecifications
		}
	}
}
