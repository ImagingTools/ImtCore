schema {
	name: UnionTest
	namespace: modsdl
	version: 1.0
	query: Query
}

type Coords{
	X: Double!
	Y: Double!
}

type PrinterSpecificationBase {
	name: String
}

type Link {
	link: ID!
}

union PrinterSpecification = PrinterSpecificationBase | Link

type PrinterBase {
	name: String
	specification: PrinterSpecification
}

type Query {
	GetSpecifications(input: Link): PrinterSpecification
}

collectionSchema {
	type Printer {
		ref: PrinterSpecification
		operations {
			get: GetSpecifications
		}
	}
}
